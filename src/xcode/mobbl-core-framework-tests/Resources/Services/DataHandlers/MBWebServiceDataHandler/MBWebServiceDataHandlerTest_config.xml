        <Domains>
        </Domains>
        <Documents>
            <Document name="WebCallParameters" dataManager="MBMemoryDataHandler">
                <Element name="UrlParam" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
                <Element name="HTTPHeaderFieldName" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
                <Element name="HTTPHeaderFieldValue" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
                <Element name="HTTPBody" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
                <Element name="WillBeReformatted" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
                <Element name="ReformattingEditPath" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
                <Element name="ReformattingReplacementValue" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
            </Document>
            <Document name="WebCallResult" dataManager="MBMockWebServiceDataHandler">
                <Element name="Result" minOccurs="1">
                    <Attribute name="text()" type="string" />
                </Element>
            </Document>
        </Documents>
    </Model>
    <Controller>
        <Actions>
        </Actions>
        <Wiring>
        </Wiring>
